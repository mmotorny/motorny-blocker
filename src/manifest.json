{
  "name": "Motorny Blocker",
  "description": "Block yourself from visiting distracting sites.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_title": "Block website"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
